<Window x:Class="Gomoku.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gomoku"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Setting" Height="200" Width="200">
    <StackPanel>
        <TextBox BorderBrush="White" FontWeight="Bold">Computer LV</TextBox>
        <StackPanel x:Name="ComLVPanel">
        </StackPanel>
        <Button Margin="2" Click="SaveSetting">Save</Button>
        <Button Margin="2" Click="CloseWindow">Close</Button>
    </StackPanel>
</Window>
